  "modified": "2023-08-11 17:00:39.510553",
  "module": "Advantisquartz",
  "name": "Purchase FMS Report",
  "prepared_report": 0,
  "query": "with po as (Select po.name,pod.material_request, po.creation, pod.parent \nfrom `tabPurchase Order` po join `tabPurchase Order Item` pod on pod.parent = po.name\nwhere po.status != \"Cancelled\"), \n grn as (select grn.name ,grn.gate_entry,grn_child.material_request,gt.creation,gt.modified,\nqt.name as \"quality\",qt.reference_type as \"purchase_name\",qt.reference_name as \"receipt_name\",qt.creation as \"creation_date\",qt.modified as \"modified_date\",\nqt.remarks,qt.inspected_by\nfrom `tabPurchase Receipt` grn join `tabPurchase Receipt Item` grn_child on grn_child.parent = grn.name\nleft outer join `tabGate Entry` gt on gt.name = grn.gate_entry\nleft outer join `tabQuality Inspection` qt on qt.name = grn_child.quality_inspection), \nmaterial_comment as(select cmt.reference_name,cmt.modified from `tabComment` cmt where cmt.reference_doctype =\"Material Request\"\nand cmt.content =\"Pending\"), \nsupplier as( Select su.name,su.workflow_state,su.transaction_date,su.creation,sud.material_request,\nsud.parent from `tabSupplier Quotation` su join `tabSupplier Quotation Item` sud on sud.parent = su.name \nwhere su.status != \"Cancelled\"), \nsupplier_comment as(select cmt.reference_name,cmt.modified \nfrom `tabComment` cmt where cmt.reference_doctype =\"Supplier Quotation\" and cmt.content =\"Approved\"), \npurchase_comment as(select cmt.reference_name,cmt.modified \nfrom `tabComment` cmt \nwhere cmt.reference_doctype =\"Purchase Order\" and cmt.content =\"To Receive and Bill\"),\n\nreceipt_comment as(select cmt.reference_name,cmt.modified\nfrom `tabComment` cmt where cmt.reference_doctype =\"Purchase Receipt\"\nand cmt.content =\"To Bill\"),\n\ngate_entry as (select gt.name, gt.modified from `tabGate Entry` gt\nwhere gt.docstatus = \"Submitted\"),\n\ngate_entrys as (select gt.name, gt.creation from `tabGate Entry` gt\nwhere gt.docstatus = \"Draft\"),\n \nactivity_grn as (select at.reference_name, at.full_name \nfrom `tabActivity Log` at where at.reference_doctype = \"Purchase Receipt\"),\n\nquality as (select qt.name,qt.reference_type,qt.reference_name,qt.creation,qt.modified,qt.remarks,qt.inspected_by\nfrom `tabQuality Inspection` qt where qt.reference_type = \"Purchase Receipt\" and\nqt.docstatus =\"Submitted\"\n)\n \n\nselect DISTINCT\nmr.name as \"Indent No\",\npo.name as \"Po No\", \ngrn.gate_entry as \"Gate No\", \ngrn.name as \"GRN NO\", \nmr.transaction_date as \"TimeStamp\", \nmr.schedule_date as \"Indent Required By\",\nmri.item_name as \"Indent Requisition Attachment\",\nmr.name as \"Requisition No\",\nmri.actual_qty as \"Available Stock\", \nmr.creation as \"Planned\", \nmaterial_comment.modified as \"Actual\",\nmr.name as \"Requisition No For Approval\",\n\"Approved Requisition\" as \"Work Status\",\nmr.remarks as \"Remarks\", \nCASE WHEN material_comment.modified IS NOT NULL THEN TIMEDIFF(material_comment.modified, mr.creation) \nELSE TIMEDIFF(NOW(), mr.creation)\nEND AS \"Time Delay\", \nsupplier.creation as \"Planned\", \nsupplier_comment.modified as \"Actual\", \nsupplier.name as \"Quatation Link\", \nsupplier.workflow_state as \"Status\",\nCASE WHEN supplier_comment.modified IS NOT NULL \nTHEN IF(supplier.creation + INTERVAL 2 DAY > supplier_comment.modified, 0, DATEDIFF( supplier_comment.modified,supplier.creation + INTERVAL 2 DAY))\nELSE IF(supplier.creation + INTERVAL 2 DAY > NOW(), 0, DATEDIFF( NOW(),supplier.creation + INTERVAL 2 DAY)) \nEND AS \"Supplier Quotation Time Delay\", \npo.creation as \"Purchase Planned Date\", \npurchase_comment.modified as \"Purchase Actual Date\", \npo.name as \"Po No\",\nCASE WHEN purchase_comment.modified IS NOT NULL \nTHEN IF(po.creation + INTERVAL 1 DAY > purchase_comment.modified, 0, DATEDIFF( purchase_comment.modified,po.creation + INTERVAL 1 DAY))\nELSE IF(po.creation + INTERVAL 1 DAY > NOW(), 0, DATEDIFF( NOW(),po.creation + INTERVAL 1 DAY))\nEND AS \"Purchase Order Time Delay\",\ngrn.creation as \"Gate Entry Planned\", \ngrn.modified as \"Gate Entry Actual\", \ngrn.gate_entry as \"Gate Entry No\",\ngrn.creation as \"Receipt Planned\",\nreceipt_comment.modified as \"Receipt Actual\",\ngrn.name as \"GRN NO\",\nactivity_grn.full_name as \"Submitted By\",\nCASE\n    WHEN receipt_comment.modified IS NOT NULL THEN\n        IF(grn.creation + INTERVAL 2 DAY > receipt_comment.modified, 0, DATEDIFF( receipt_comment.modified,grn.creation + INTERVAL 2 DAY))\n    ELSE\n        IF(grn.creation + INTERVAL 2 DAY > NOW(), 0, DATEDIFF( NOW(),grn.creation + INTERVAL 2 DAY))\nEND AS \"Receipt Time Delay\",\ngrn.creation_date as \"Quality Planned\",\ngrn.modified_date as \"Quality Actual\",\ngrn.remarks as \"Remarks\",\ngrn.inspected_by as \"Submited by\",\nCASE\n    WHEN grn.modified_date IS NOT NULL THEN\n        IF(grn.creation_date + INTERVAL 2 DAY > grn.modified_date, 0, DATEDIFF( grn.modified_date,grn.creation_date + INTERVAL 2 DAY))\n    ELSE\n        IF(grn.creation_date + INTERVAL 2 DAY > NOW(), 0, DATEDIFF( NOW(),grn.creation_date + INTERVAL 2 DAY))\nEND AS \"Quality Time Delay\"\n\nfrom `tabMaterial Request` mr join `tabMaterial Request Item` mri on mr.name = mri.parent\njoin po on mr.name = po.material_request \njoin grn on mr.name = grn.material_request \njoin material_comment on material_comment.reference_name = mr.name\njoin supplier on supplier.material_request = mr.name\njoin supplier_comment on supplier_comment.reference_name = supplier.name \njoin purchase_comment on purchase_comment.reference_name = po.name\nleft outer join receipt_comment on receipt_comment.reference_name = grn.name\nleft outer join activity_grn on activity_grn.reference_name = grn.name\nleft outer join quality on quality.reference_name=grn.name\n",
  "ref_doctype": "Request for Quotation",
