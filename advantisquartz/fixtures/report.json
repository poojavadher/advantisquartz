  "name": "Rejected  Material List",
  "prepared_report": 0,
  "query": "select \ngrn.supplier_name,\nprt.purchase_order,\npo.transaction_date,\ngrn.gate_entry,\ngt.gate_in_date,\ngt.gate_in_time,\npr.name,\npr.posting_date,\nprt.item_code,\nprt.item_name,\npod.qty as \"Po Qty\",\nprt.qty,\nprt.rejected_qty,\nprt.reject_issue,\nprt.quality_inspection\nfrom `tabPurchase Receipt` grn join\n`tabPurchase Receipt Item` prt on grn.name = prt.parent\nleft outer join `tabPurchase Order` po on prt.purchase_order = po.name\nleft outer join `tabGate Entry` gt on grn.gate_entry = gt.name\nleft outer join `tabPurchase Order Item` pod on po.name = pod.parent\n left outer join `tabPurchase Invoice Item` poi on grn.name = poi.purchase_receipt\n left outer join `tabPurchase Invoice` pr on pr.name = poi.parent\n left outer join `tabQuality Inspection` qc on prt.quality_inspection = qc.name \n Where\n(\n       DATE(grn.posting_date) between %(from_date) s\n       and %(to_date) s\n   )\n   and\n   prt.rejected_qty",
  "ref_doctype": "Purchase Receipt",
